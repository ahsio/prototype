{
    "name": "fabpot/silex-skeleton",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.1",
        "silex/web-profiler": "1.0.*",
        "symfony/security": ">=2.4,<2.6-dev",
        "symfony/debug": ">=2.4,<2.6-dev",
        "symfony/finder": ">=2.4,<2.6-dev",
        "symfony/monolog-bridge": ">=2.4,<2.6-dev",
        "symfony/doctrine-bridge": ">=2.4,<2.6-dev",
        "symfony/process": ">=2.4,<2.6-dev",
        "symfony/serializer": ">=2.4,<2.6-dev",
        "symfony/twig-bridge": ">=2.4,<2.6-dev",
        "twig/twig": ">=1.8.0,<2.0-dev",
        "doctrine/dbal": "~2.2",
        "swiftmailer/swiftmailer": "5.*",
        "monolog/monolog": "~1.4,>=1.4.1",
        "knplabs/github-api": "~1.2@dev",
        "dflydev/doctrine-orm-service-provider": "1.0.*",
        "easybook/geshi": "1.0.8.12",
        "hipchat/hipchat-php": ">=1.0.0"
    },
    "minimum-stability": "stable",
    "require-dev":{
        "phpspec/prophecy": "1.0.*"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
